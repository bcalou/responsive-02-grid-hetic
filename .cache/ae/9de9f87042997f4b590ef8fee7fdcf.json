emplate-columns: repeat(18, 1fr);\r\n      grid-template-rows: repeat(7, 80px);\r\n    }\r\n}\r\n  \r\n.el {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n    background-color: lightgrey;\r\n    border: 2px solid darkgrey;\r\n    font-family: Helvetica, sans-serif;\r\n\r\n    @include large {\r\n      @for $i from 1 through 18{\r\n        &[data-group=\"#{$i}\"]{\r\n          grid-column: #{$i};\r\n        }\r\n      }\r\n\r\n      @for $i from 1 through 7{\r\n        &[data-period=\"#{$i}\"]{\r\n          grid-row: #{$i};\r\n        }\r\n      }\r\n    }\r\n}\r\n  \r\n.el__number {\r\n    font-size: 16px;\r\n\r\n    @include large {\r\n      font-size: 12px;\r\n    }\r\n}\r\n  \r\n.el__symbol {\r\n    font-size: 24px;\r\n\r\n    @include large {\r\n      font-size: 14px;\r\n      font-weight: bold;\r\n    }\r\n}\r\n  \r\n.el__name {\r\n    font-size: 10px;\r\n\r\n    @include large {\r\n      font-size: 8px;\r\n    }\r\n}\r\n\r\n$types: alkaliMetal lanthanide nobleGas transitionMetal postTransitionMetal alkalineEarthMetal actinide metalloid otherNonMetal;\r\n$step: 360/ length($types);\r\n  \r\n@each $type in $types {\r\n  .el--#{$type} {\r\n    $index: index($types, $type);\r\n    background-color: hsl($step*$index, 100%, 40%, 0.7); \r\n    border: 3px solid hsl($step*$index, 100%, 40%);\r\n    color: black;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"8ffcf0e3ed7df31586dbccc00c894a21","cacheData":{"env":{}}}